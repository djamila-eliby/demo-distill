file = {:C\:/Users/DJeli/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gibson-Smith et al. - 2018 - Diet quality in persons with and without depressive and anxiety disorders.pdf:pdf},
issn = {18791379},
journal = {Journal of Psychiatric Research},
keywords = {Alternative healthy eating index,Anxiety,Depression,Diet,Mediterranean diet,Nutrition},
number = {August},
pages = {1--7},
publisher = {Elsevier},
title = {{Diet quality in persons with and without depressive and anxiety disorders}},
url = {https://doi.org/10.1016/j.jpsychires.2018.09.006},
