issn = {20726643},
journal = {Nutrients},
keywords = {Alternate Mediterranean diet score,Depressive symptoms,Dietary patterns,Mediterranean diet,Mental health},
number = {2},
pages = {1--13},
pmid = {35057462},
title = {{Adherence to a Mediterranean Diet Is Associated with Lower Depressive Symptoms among U.S. Adults}},
volume = {14},
year = {2022}
}
