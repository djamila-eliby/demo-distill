isbn = {2016041720140},
issn = {22122672},
journal = {Journal of the Academy of Nutrition and Dietetics},
keywords = {Brain-derived neurotrophic factor,Depression,Olive oil,Salivary cortisol},
publisher = {Elsevier Inc},
title = {{Extra-Virgin Olive Oil Improves Depression Symptoms Without Affecting Salivary Cortisol and Brain-Derived Neurotrophic Factor in Patients With Major Depression: A Double-Blind Randomized Controlled Trial}},
url = {https://doi.org/10.1016/j.jand.2021.07.016},
