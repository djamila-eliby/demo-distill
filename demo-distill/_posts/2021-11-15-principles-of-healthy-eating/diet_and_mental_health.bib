doi = {10.1097/PSY.0000000000000673},
file = {:C\:/Users/DJeli/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Firth et al. - 2019 - The Effects of Dietary Improvement on Symptoms of Depression and Anxiety A Meta-Analysis of Randomized Controlled.pdf:pdf},
