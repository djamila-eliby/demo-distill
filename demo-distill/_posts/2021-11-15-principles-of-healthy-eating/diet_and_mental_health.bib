doi = {10.1016/j.jad.2013.01.014},
file = {:C\:/Users/DJeli/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lopresti, Hood, Drummond - 2013 - A review of lifestyle factors that contribute to important pathways associated with major depression D.pdf:pdf},
