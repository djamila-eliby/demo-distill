number = {2},
pages = {537--544},
pmid = {26453120},
publisher = {Elsevier},
title = {{A lifestyle intervention for primary care patients with depression and anxiety: A randomised controlled trial}},
url = {http://dx.doi.org/10.1016/j.psychres.2015.10.001},
volume = {230},
year = {2015}
